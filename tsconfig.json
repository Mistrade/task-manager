{
  "compilerOptions": {
    "target": "ES6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "baseUrl": "./",
    "paths": {
      "@core/*": [
        "./src/core/*"
      ],
      "@planner/*": [
        "./src/pages/planner/*"
      ],
      "@api/*": [
        "./src/store/api/*"
      ],
      "@redux-hooks/*": [
        "./src/store/hooks"
      ],
      "@redux/*": [
        "./src/store/*"
      ],
      "@selectors/*": [
        "./src/store/selectors/*"
      ],
      "@contacts/*": [
        "./src/pages/Contacts/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@planner-reducer/*": [
        "./src/store/reducers/planner/*"
      ],
      "@src/*": [
        "./src/*"
      ]
    },
    "rootDirs": []
  },
  "include": [
    "./src"
  ],
  "exclude": [
    "node_modules"
  ]
}
